{
  "name": "splitter",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "bun run ./build/index.js",
    "dev": "vite dev",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "register-commands": "run-script-os",
    "register-commands:default": "REGISTER_COMMANDS=1 bun --bun run dev -- --open",
    "register-commands:win32": "set REGISTER_COMMANDS=1 && bun --bun run dev -- --open"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.15.7",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "bun-types": "^1.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "run-script-os": "^1.1.6",
    "svelte": "^3.58.0",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.1"
  },
  "dependencies": {
    "discord.js": "^14.9.0",
    "http-status-codes": "^2.2.0",
    "tempy": "^3.0.0"
  }
}
