{
	"name": "splitter-web-interface",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "pnpm run build && pnpm run host",
		"dev": "vite dev",
		"build": "vite build",
		"host": "vite preview --host --port 80",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"register-commands": "run-script-os",
		"register-commands:default": "REGISTER_COMMANDS=1 npm run dev -- --open",
		"register-commands:win32": "set REGISTER_COMMANDS=1 && npm run dev -- --open"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/better-sqlite3": "^7.6.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"run-script-os": "^1.1.6",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"better-sqlite3": "^8.1.0",
		"cors-anywhere": "^0.4.4",
		"discord.js": "^14.7.1",
		"http-status-codes": "^2.2.0"
	}
}
